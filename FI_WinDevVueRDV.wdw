     p_codes :
      -
        code : |1+
         // Procédure appelée avant chaque dessin d'un rendez-vous de champ Agenda, Planning, TimeLine
         // Ce code est ré-exécuté pour CHAQUE affichage d'un rendez-vous 
         procédure FI_WINDEV_VueRDV( gEvenementOuRDV est un rendezvous )
         //EvenementOuRDV : Rendez-vous ou Evenement a dessiner
         
         // => SourceVersEcran
         LIB_Détail 						= gEvenementOuRDV.Contenu
         LIB_TitreRDV 					= gEvenementOuRDV.Titre
         LIB_FondTitreRDV..couleurFond	= gEvenementOuRDV.CouleurFond
         IMG_Image1..Valeur 				= gEvenementOuRDV.Image
         IMG_Important..Visible			= (gEvenementOuRDV.Importance <> 0)
         IMG_REPETITION..Visible			= gEvenementOuRDV.AvecRépétition
         
         
         // cas ou le RDV est petit et qu'on a un pictogramme
         si MoiMême..Hauteur <= 2*LIB_FondTitreRDV..Hauteur _et_  gEvenementOuRDV.Image<>"" ALORS
         	// on met le libellé a coté du titre
         	IMG_Image1..Y = LIB_TitreRDV..Y
         	LIB_TitreRDV..X = IMG_Image1..x + IMG_Image1..Largeur
         FIN
      -
        type : 15
      -
        type : 16
