  p_codes :
   -
     code : |1+
      PROCEDURE ETAT_PLANNING(LOCAL gdDateDebut est une Date, LOCAL gdDateFin est une Date)
      
      POUR TOUT TACHE 
      	bOK est un booléen = Faux
      	
      	// Si la date de début de la tâche est plus petite que la date de début, il faut alors que la date de fin soit au moins plus grand que la date de début
      	SI TACHE.DATEDEBUT <= gdDateDebut ET TACHE.DATEFIN >= gdDateDebut ALORS
      		bOK = Vrai
      	FIN
      	
      	// Si la date de début de la tâche est plus grande que la date de début, il faut alors que la date de début de la tache soit plus petite que la date de fin
      	SI TACHE.DATEDEBUT >= gdDateDebut ET TACHE.DATEDEBUT <= gdDateFin ALORS
      		bOK = Vrai
      	FIN
      	
      	SI bOK ALORS
      		// Variable
      		dDateDebut est une Date = TACHE.DATEDEBUT
      		dDateFin   est une Date = TACHE.DATEFIN
      		
      		SI TACHE.DATEDEBUT < gdDateDebut ALORS
      			dDateDebut = gdDateDebut
      		FIN
      		
      		SI TACHE.DATEFIN > gdDateFin ALORS
      			dDateFin = gdDateFin
      		FIN
      		
      		HLitRecherchePremier(PERSONNE,IDPERSONNE,TACHE.IDPERSONNE)
      		HLitRecherchePremier(TYPE,IDTYPE,TACHE.IDTYPE)
      		
      		PlanningAjouteRendezVous(PLN_PLANNING,PERSONNE.NOM,TACHE.LIBTACHE,dDateDebut,dDateFin,TYPE.LIBTYPE)
      	FIN	
      FIN
      
      
