  procedures :
   -
     name : AffichePlanning
     procedure_id : 1767071376638661976
     type_code : 14
     code : |1+
      PROCEDURE AffichePlanning(sListeQui)
      
      // Variables
      i est un entier
      IDPersonne sont des entiers  
      tmpRDV     est un RendezVous
      
      
      // Réinitialise
      PlanningSupprimeTout(PLN_PRINCIPALE)
      
      // Initialisation
      i++
      IDPersonne=Val(ExtraitChaîne(sListeQui,i))
      
      // Pour chaque personne sélectionnée
      TANTQUE IDPersonne <> 0
      	// Recherche des tâches sur la période donné, pour la personne en cours	
      	hfiltre(TACHE,IDPERSONNE,IDPersonne)
      	HLitPremier(TACHE)
      	
      	// Si la personne a des taches on l'insère dans la liste et on boucle insérer ses taches
      	SI HTrouve(TACHE) ALORS	
      		// On filtre les informations de la personne
      		hfiltre(PERSONNE,IDPERSONNE,tache.IDPERSONNE)
      		HLitPremier(PERSONNE)
      		
      		// On a trouvé la personne, on l'affiche dans le planning
      		PlanningAjouteRessource(PLN_PRINCIPALE,PERSONNE.NOM + gLien(TACHE.IDPersonne))
      
      		// Tant qu'il y a des taches donnés pour cette personne dans le planning, on les affiche
      		TANTQUE PAS HEnDehors(TACHE)
      			
      			// On filtre les informations liées à la tache
      			hfiltre(TYPE,IDTYPE,tache.IDTYPE)
      			HLitPremier(TYPE)
      			
      			HFiltre(LIEU,IDLIEU,TACHE.IDLIEU)
      			HLitPremier(LIEU)
      			
      			// Ajout de la plage
      			tmpRDV.Ressource   = TACHE.IDPersonne
      			tmpRDV.Titre       = TACHE.LIBTACHE
      			tmpRDV.DateDébut   = TACHE.DATEDEBUT
      			tmpRDV.DateFin     = TACHE.DATEFIN
      			tmpRDV.Catégorie   = TYPE.LIBTYPE
      			tmpRDV.Contenu     = TACHE.CONTENUTACHE
      			tmpRDV.CouleurFond = TYPE.COULTYPE
      			tmpRDV.Note        = TACHE.IDTACHE
      			tmpRDV.Lieu        = LIEU.LIBLIEU
      			tmpRDV.Bulle       = TYPE.LIBTYPE + " - " + LIEU.LIBLIEU
      			tmpRDV.JournéeEntière = TACHE.JOURENTIER
      			PlanningAjouteRendezVous(PLN_PRINCIPALE,tmpRDV)
      			
      			HLitSuivant(TACHE)	
      		FIN
      	// Sinon on insère juste la personne dans la liste si elle existe
      	SINON
      		HLitRecherche(PERSONNE,IDPERSONNE,IDPersonne)
      		SI HTrouve() ALORS
      			HLitRecherche(GROUPES,IDGROUPE,PERSONNE.IDGROUPE)
      			// Ajout de la personne dans le planning alors qu'elle n'a pas de tâches sur la période
      			PlanningAjouteRessource(PLN_PRINCIPALE,PERSONNE.NOM + gLien(PERSONNE.IDPERSONNE))		
      		FIN	
      	FIN
      	
      	// On passe à la personne suivante
      	i++
      	IDPersonne=Val(ExtraitChaîne(sListeQui,i))	
      FIN
      
      HDésactiveFiltre(LIEU)
      HDésactiveFiltre(TYPE)
      HDésactiveFiltre(TACHE)
      HDésactiveFiltre(PERSONNE)
      
      
      
     type : 458752
   -
     name : TriListe
     procedure_id : 1767071376638793048
     type_code : 14
     code : |1+
      Procédure TriListe(sListeQui)
      
      // Tri la liste des personnes en ordre alphabétique
      
      memcree("ZMemTrie")
      i, IDPersonne sont des entiers  
      
      // Parcours de la liste actuelle des n°s
      boucle
      	i++
      	IDPersonne=val(ExtraitChaine(sListeQui,i))
      	// Recherche du nom de la personne et chargement de la zone mémoire pour le tri
      	HLitRecherche(PERSONNE,IDPERSONNE,IDPersonne)
      	si HTrouve() alors Memajoute("ZMemTrie",PERSONNE.NOM,PERSONNE.IDPERSONNE)
      	si IDPersonne=0 alors sortir
      fin
      
      // Tri mémoire
      MemTrie("ZMemTrie")
      
      // Réinitialisation de la liste
      sListeQui=""
      pour i=1 _à_ memoccurrence("ZMemTrie")
      	si pas sListeQui~="" alors sListeQui+=tab
      	sListeQui+=memrecupere("ZMemTrie",i)
      FIN
      
      // Libération de la zone mémoire
      MemSupprimeTout("ZMemTrie")
      
