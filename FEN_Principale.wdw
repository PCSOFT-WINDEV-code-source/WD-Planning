   -
     name : NouvelleTache
     procedure_id : 1767731702092316211
     type_code : 14
     code : |1+
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      //NouvelleTache ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      // Exemple :
      // Indiquez ici un exemple d'utilisation.
      //
      Procédure NouvelleTache()
      
      // Appel de l'assistant
      nIDTacheCréée est un entier = ouvre(FEN_TacheFiche)
      
      SI nIDTacheCréée <> 0 ALORS
      	// Recherche de la tache créée pour l'afficher
      	HLitRecherche(TACHE,IDTACHE,nIDTacheCréée)
      	
      	// Personne déjà affichée dans le planning ?
      	SI Position(gsListeQui,NumériqueVersChaîne(TACHE.IDPERSONNE)) < 1 ALORS
      		// Ajout de la personne dans la liste
      		SI PAS gsListeQui~="" ALORS gsListeQui += TAB
      		gsListeQui += TACHE.IDPERSONNE
      		
      		// Tri de la liste (n°s des personnes) sur le nom
      		TriListe(gsListeQui)
      	FIN
      	
      	// Tracé du planning
      	AffichePlanning(gsListeQui)
      FIN
      
      
