        name : OPT_Importer_un_fichier_iCalendar
        identifier : 0x264b39b802370651
        internal_properties : 
        code_elements :
         type_code : 26
         p_codes :
          -
            internal_properties : CAAAAAgAAADQeTMQm6RsKecOrU4JhDojhD5z7axwvRwo9P4nV7zql+gSEYS0G9Q/uw2NV6p4EnslkgArjJusmlyh89tTplyDglRURQklf1cMUH8xbns8e30lAzRbO2/kgnDP0OpPbNfbOJzQEwONinocxML+fo7vICtUEVhHUxMi1MPL6aXsoFFqx1zBP1wdouW1KEODx4WU
            code : |1+
             // ID de la ressource
             sRessource est une chaine
             // Chemin du fichier
             sFichier est une chaîne
             // Chaine contenant le planning au format ics
             sICalendar est une chaine ansi
             
             
             // Ouvre le sélecteur de fichiers
             sFichier = fSélecteur("", "", "Importez votre planning ...", "Fichier iCalendar" + TAB + "*.ics", "*.ics", fselOuvre)
             
             SI sFichier = "" ALORS
             	RETOUR
             FIN
             
             // On récupère le contenu du fichier
             sICalendar = fChargeTexte(sFichier,foAnsi)
             
             // On cherche la ressource 
             sRessource = ouvre(FEN_SelectionPersonneiCalendar)
             
             // On vérifie si la ressource est correcte
             SI sRessource="" ALORS
             	RETOUR
             FIN
             
             // On ajoute le fichier ics aux fichiers de données
             tabRDV est un tableau de RendezVous = iCalendarVersRendezVous(sICalendar)
             SI PAS ErreurDétectée ALORS
             	POUR TOUT ELEMENT rdv DE tabRDV
             		// On RAZ le fichier
             		HRAZ(TACHE)
             		
             		// On affecte toutes les rubriques
             		tache.CONTENUTACHE=rdv.Contenu
             		TACHE.DATEDEBUT= rdv.DateDébut
             		TACHE.DATEFIN = rdv.DateFin
             		
             		si HLitRecherchePremier(LIEU,LIBLIEU,rdv.Lieu)=vrai ALORS
             			TACHE.IDLIEU=LIEU.IDLIEU	
             		FIN
             		
             		TACHE.IDPERSONNE=sRessource
             				
             		SI HLitRecherchePremier(TYPE,LIBTYPE,rdv.Catégorie)=Vrai ALORS
             			TACHE.IDTYPE=type.IDTYPE
             		sinon
             			// On met le type autre, si on ne trouve pas de type
             			HLitRecherchePremier(TYPE,LIBTYPE,"Autre")
             			TACHE.IDTYPE=TYPE.IDTYPE
             		FIN
             		
             		TACHE.LIBTACHE = rdv.Titre
             		
             		// Par défaut les RDV importés sont prévu et non confirmé
             		TACHE.ETATTACHE=1
             		
             		// On ajoute l'enregistrement
             		si HAjoute(TACHE) = faux ALORS
             			erreur(ErreurInfo(errComplet))
             		FIN
             	FIN
             FIN
             
             
             // Tracé du planning
             AffichePlanning(gsListeQui)
             
             // On se positionne au début de l'agenda
             PlanningPositionDateHeure(PLN_PRINCIPALE,gdDateDebut)
             
             ToastAffiche(<§$0007§>)
             
            type : 32
