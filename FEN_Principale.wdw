   -
     name : _DonnéesDeTest
     procedure_id : 1884737683639590241
     type_code : 14
     code : |1+
      // Résumé : Génère des données de test si besoin
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      PROCEDURE _DonnéesDeTest()
      
      // S'il y a des tâches, on ne fait rien
      si HNbEnr(TACHE) > 0 ALORS
      	retour
      FIN
      
      // Question
      si ouinon(<§$0009§>) = NON ALORS
      	// Pas de données de test
      	retour
      FIN
      
      // Génération des données
      dhAujourdhui est une dateheure
      dhAujourdhui..PartieDate = DateDuJour()
      dhAujourdhui..Heure = 8
      dhDebut, dhFin est une dateheure
      
      
      // Une tâche
      dhDebut = dhAujourdhui
      dhDebut..Jour += 6
      dhFin = dhAujourdhui
      dhFin..Jour += 16
      _AjouteUneTache(dhDebut,dhFin,<§$0015§>,1,28,7,13,<§$0016§>+rc+ <§$0017§>,0)
      
      
      // Une tâche
      dhDebut = dhAujourdhui
      dhDebut..Jour += 5
      dhFin = dhAujourdhui
      dhFin..Jour += 11
      _AjouteUneTache(dhDebut,dhFin,<§$0014§>,1,19,9,12,"Vacances en Guadeloupe",0)
      
      // Une tâche
      dhDebut = dhAujourdhui
      dhDebut..Jour += 1
      dhFin = dhAujourdhui
      dhFin..Jour += 6
      _AjouteUneTache(dhDebut,dhFin,<§$0013§>,2,12,28,13,"",0)
      
      // Une tâche
      dhDebut = dhAujourdhui
      dhDebut..Jour += 11
      dhFin = dhAujourdhui
      dhFin..Jour += 19
      _AjouteUneTache(dhDebut,dhFin,<§$0012§>,1,27,31,11,"Permanence pour la maintenance des serveurs.",0)
      
      // Une tâche
      dhDebut = dhAujourdhui
      dhDebut..Jour += 16
      dhFin = dhAujourdhui
      dhFin..Jour += 22
      _AjouteUneTache(dhDebut,dhFin,<§$0011§>,2,26,40,3,"Préparation de la démonstration pour les clients les plus importants. Penser à envoyer les serveurs à Paris.",0)
      
      // Une tâche
      dhDebut = dhAujourdhui
      dhDebut..Jour += 7
      dhFin = dhAujourdhui
      dhFin..Jour += 19
      _AjouteUneTache(dhDebut,dhFin,<§$0010§>,2,4,16,7,"Rattrapage des heures supplémentaires.",0)
      
      // Une tâche
      dhDebut = dhAujourdhui
      dhDebut..Jour += 11
      dhFin = dhAujourdhui
      dhFin..Jour += 26
      _AjouteUneTache(dhDebut,dhFin,<§$000f§>,1,9,31,8,"",0)
      
      // Une tâche
      dhDebut = dhAujourdhui
      dhFin = dhAujourdhui
      dhFin..Jour += 5
      _AjouteUneTache(dhDebut,dhFin,<§$000e§>,1,23,28,7,"Nouvelles versions à empaqueter.Version française et anglaise.",1)
      
      // Une tâche
      dhDebut = dhAujourdhui
      dhDebut..Jour += 14
      dhFin = dhAujourdhui
      dhFin..Jour += 21
      _AjouteUneTache(dhDebut,dhFin,<§$000d§>,1,32,20,12,"Relookage des exemples livrés avec le produit afin de les mettre à la norme de l'entreprise.",0)
      
      // Une tâche
      dhDebut = dhAujourdhui
      dhDebut..Jour += 14
      dhFin = dhAujourdhui
      dhFin..Jour += 33
      _AjouteUneTache(dhDebut,dhFin,<§$000c§>,2,30,16,12,"Vacances annuelles.",0)
      
      
      
      
      	// Ajoute une tâche dans le fichier
      	Procédure interne _AjouteUneTache(dhDebutTache est une dateheure, dhFinTache est une dateheure, sLibelle est une chaîne, nEtatTache est un entier, nIDPersonne est un entier, nIDType est un entier,...
      	nIDLieu est un entier, sContenuTache est une chaine, nJourEntier est un entier)
      	
      	HRAZ(TACHE)
      	TACHE.DATEDEBUT = dhDebutTache
      	TACHE.DATEFIN = dhFinTache
      	TACHE.LIBTACHE = sLibelle
      	TACHE.ETATTACHE = nEtatTache
      	TACHE.IDPERSONNE = nIDPersonne
      	TACHE.IDTYPE = nIDType
      	TACHE.IDLIEU = nIDLieu
      	TACHE.CONTENUTACHE = sContenuTache
      	TACHE.JOURENTIER = nJourEntier
      	HAjoute(TACHE)
      	fin
