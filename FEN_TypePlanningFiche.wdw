  p_codes :
   -
     code : |1+
      procedure FEN_TypePlanningFiche(sDu="",sAu="",sListeQui="")
      
      GLOBAL
      	i est un entier
      	s est une chaîne
      	nIDPersonne est un entier  
      
      // Personnes sélectionnées
      si pas sListeQui~=""
      	boucle
      		i++
      		s=ExtraitChaine(sListeQui,i)
      		si s=eot alors sortir
      		nIDPersonne=val(s)
      		HLitRecherche(PERSONNE,IDPERSONNE,nIDPersonne)
      		si HTrouve() alors ListeAjoute(LISTE_Personne,PERSONNE.NOM+repete(" ",100)+tab+PERSONNE.IDPERSONNE)
      	FIN
      fin
      
      
      // Période par défaut
      si sDu~="" alors sDu=datesys()
      si sAu~="" alors sAu=EntierVersDate(DateVersEntier(sDu)+7)
