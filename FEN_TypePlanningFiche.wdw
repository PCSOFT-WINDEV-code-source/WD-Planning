     p_codes :
      -
        code : |1+
         
         // Variables
         RacineTV est une chaîne = <§$0000§>
         BrancheTV est une chaine
         
         // Initialisation
         arbreajoute(ARBRE_Personne,RacineTV,"GROUPE.ICO","GROUPE.ICO","0")
         
         // Groupes de personnes
         i=0
         Boucle
         	i++
         	HLit(GROUPES,i)
         	si HEnDehors() alors Sortir
         	si (HEtat()=HEtatActif) alors
                 // Création du groupe dans le TreeView
         		BrancheTV=SansEspace(GROUPES.LIBGROUPE)
         		Arbreajoute(ARBRE_Personne,RacineTV+tab+BrancheTV,adefaut,adefaut,"G"+GROUPES.IDGROUPE)
         		arbrederoule(ARBRE_Personne,RacineTV+tab+BrancheTV)
         		// Recherche des personnes affectées à ce groupe
         		HLitRecherche(PERSONNE,IDGROUPE,GROUPES.IDGROUPE)
         		tantque HTrouve()
         			arbreajoute(ARBRE_Personne,RacineTV+tab+BrancheTV+tab+SansEspace(PERSONNE.NOM),"SUIVAN5-.BMP","SUIVAN5-.BMP","P"+PERSONNE.IDPERSONNE)
         			HLitSuivant(PERSONNE,IDGROUPE)
         		FIN
         	fin
         Fin
         
        type : 14
