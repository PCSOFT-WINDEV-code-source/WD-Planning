        type : 35
        enabled : false
      -
        type : 19
        enabled : false
      -
        type : 20
        enabled : false
      -
        code : |1+
         // Récupération de l'ID de la branche sélectionnée
         ID est une chaîne = arbrerecupereidentifiant(ARBRE_Personne,arbreselect(ARBRE_Personne))
         si ID[[1]]="P"	// Personne
         	HLitRecherche(PERSONNE,IDPERSONNE,val(ID[[2à]]))
         	si ListeCherche(LISTE_Personne,PERSONNE.NOM+repete(" ",100)+tab+PERSONNE.IDPERSONNE)=-1 alors
         		ListeAjoute(LISTE_Personne,PERSONNE.NOM+repete(" ",100)+tab+PERSONNE.IDPERSONNE)
         	fin
         FIN
         
