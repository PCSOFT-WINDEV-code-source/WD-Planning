  procedures :
   -
     name : MAJTache
     procedure_id : 1767082101241998558
     type_code : 14
     code : |1+
      Procédure MAJTache()
      
      // Modification ?
      SI gnIDTache <> 0 ALORS
      	HLitRecherche(TACHE,IDTACHE,gnIDTache)
      	si pas HTrouve() alors retour
      SINON
      	HRAZ(TACHE)
      FIN
      	
      // Récupération des données saisies et contrôle
      TACHE.DATEDEBUT    = SAI_DATE_DEBUT + SAI_HEURE_DEBUT
      TACHE.DATEFIN      = SAI_DATE_FIN + SAI_HEURE_FIN
      TACHE.LIBTACHE     = SAI_Tache
      TACHE.ETATTACHE    = SEL_EtatTache
      TACHE.CONTENUTACHE = SAI_CONTENU
      tache.JOURENTIER   = INT_JourEntier
      
      si COMBO_Personne > 1 alors
      	TACHE.IDPERSONNE = COMBO_Personne..Valeur
      sinon
      	erreur(<§$0003§>)
      	RepriseSaisie(COMBO_Personne)
      fin
      
      si COMBO_Type > 1 alors
      	TACHE.IDTYPE = COMBO_Type..Valeur
      sinon
      	erreur(<§$0004§>)
      	RepriseSaisie(COMBO_Type)
      fin
      
      si COMBO_Lieu > 1 alors
      	TACHE.IDLIEU = COMBO_Lieu..Valeur
      sinon
      	TACHE.IDLIEU = 0
      fin
      
      SI gnIDTache <> 0 ALORS
      	// Enregistrement
      	HModifie(TACHE)
      	SI HErreurIntégrité() ALORS
      		Erreur(HErreurInfo())
      		RepriseSaisie()
      	FIN	
      SINON
      	HAjoute(TACHE)
      FIN
     type : 458752
   -
     name : ControleSaisie
     procedure_id : 1767082101242260702
     type_code : 14
     code : |1-
      procédure ControleSaisie()
      
      // Vérifie que les champs principaux sont saisis
      SI PAS DateValide(SAI_DATE_DEBUT) ALORS
      	Erreur(<§$000c§>)
      	RENVOYER Faux
      FIN
      
      SI PAS DateValide(SAI_DATE_FIN) ALORS
      	Erreur(<§$000d§>)
      	RENVOYER Faux
      FIN
      
      SI PAS HeureValide(SAI_HEURE_DEBUT) ALORS
      	Erreur(<§$000e§>)
      	RENVOYER Faux
      FIN
      
      SI PAS HeureValide(SAI_HEURE_FIN) ALORS
      	Erreur(<§$000f§>)
      	RENVOYER Faux
      FIN
      
      SI SAI_Tache ~= "" ALORS
      	Erreur(<§$0010§>)
      	RENVOYER Faux
      FIN
      
      RENVOYER Vrai
     type : 458752
   -
     name : InitialiseFormulaire
     procedure_id : 2071719080595445876
     type_code : 14
     code : |1-
      // Résumé : Initialise les champs de la fenêtre
      // Syntaxe :
      // InitialiseFormulaire ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      // Exemple :
      // Indiquez ici un exemple d'utilisation.
      //
      PROCEDURE InitialiseFormulaire()
      
      // Variables
      dDateDuJour est une Date = DateDuJour()
      
      // Modification ?
      Si gnIDTache <> 0 ALORS
      	HLitRecherchePremier(TACHE,IDTACHE,gnIDTache)
      	dbgAssertion(HTrouve(TACHE),"Tache introuvable")
      	
      	// Initialise les champs
      	FichierVersEcran(FEN_TacheFiche,TACHE)
      	
      	SAI_DATE_DEBUT  = TACHE.DATEDEBUT..PartieDate
      	SAI_DATE_FIN    = TACHE.DATEFIN..PartieDate
      	SAI_HEURE_DEBUT = TACHE.DATEDEBUT..PartieHeure
      	SAI_HEURE_FIN   = TACHE.DATEFIN..PartieHeure
      	
      	// Gestion des tâche à la journée
      	SI Tache.JOURENTIER = Vrai alors
      		SAI_HEURE_DEBUT..Etat = Grisé
      		SAI_HEURE_FIN..Etat = Grisé
      	FIN	
      	
      	// Suppression possible
      	BTN_SUPPRIMER..Visible = Vrai
      	FEN_TacheFiche..Titre = "Modification d'une tâche existante"
      SINON
      	// Valeurs par défaut
      	SAI_DATE_DEBUT  = dDateDuJour ; dDateDuJour..Jour += 7
      	SAI_DATE_FIN    = dDateDuJour
      	SAI_HEURE_DEBUT = "0800"
      	SAI_HEURE_FIN   = "1800"
      	
      	BTN_SUPPRIMER..Visible = Faux
      	FEN_TacheFiche..Titre = "Nouvelle tâche"
      	
      	ListeSelectPlus(COMBO_Lieu,2)
      	ListeSelectPlus(COMBO_Personne,2)
      	ListeSelectPlus(COMBO_Type,2)
      FIN
      
      ExécuteTraitement(COMBO_Type,trtSélection)
      FEN_TacheFiche..ValeurRenvoyée = Faux
     type : 458752
